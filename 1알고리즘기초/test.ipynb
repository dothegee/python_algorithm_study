     ]
    }
   ],
   "source": [
    "#실습1-7\n",
    "#1부터 n까지 정수의 합 구하기1(while 문)\n",
    "print(\"1부터 n까지 정수의 합\")\n",
    "n = int(input(\"n의 값을 입력 : \"))\n",
    "\n",
    "sum = 0\n",
    "i = 1\n",
    "\n",
    "while i <= n: #i가 n 보다 작거나 같을 동안 반복\n",
    "    sum += i\n",
    "    i += 1\n",
    "\n",
    "print(f\"{n}까지 정수 합은 {sum}\")\n"
   ]
  },
